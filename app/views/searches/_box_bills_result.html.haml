- @bills.each do |bill|
  %div.row
    %div.col-md-1
      - if ! bill.icon.blank?
        = image_tag("statusicon/" + bill.icon, class:"hidden-xs")
    %div.col-md-10
      %h5
        = link_to bill.title, t('routes.bills') + '/' + bill.uid, :onclick => "ga('send','paperwork','bill', '"+bill.uid+"')"
      %p 
        = bill.abstract
    %div.col-md-1
      %a{href: "/"}
        0
        %i.fa.fa-star-o
%div.text-right
  - if ! params[:q].blank?
    = link_to t('search.all_results'), searches_bills_path + '/?q=' + params[:q] + '&bills=1', :class => "btn btn-default"